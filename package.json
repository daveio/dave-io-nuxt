    "@nuxtjs/color-mode": "^3.5.2",
    "@pinia/nuxt": "0.11.0",
    "@tanstack/vue-table": "8.21.3",
    "@types/js-yaml": "4.0.9",
    "@unhead/vue": "2.0.10",
    "chart.js": "4.4.9",
    "cloudflare": "4.3.0",
    "date-fns": "4.1.0",
    "fast-xml-parser": "5.2.3",
    "jose": "6.0.11",
    "js-yaml": "4.1.0",
    "jsonc-parser": "3.3.1",
    "lodash-es": "4.17.21",
    "nuxt": "3.17.4",
    "pinia": "3.0.2",
    "recharts": "2.15.3",
    "typescript": "5.8.3",
    "uuid": "11.1.0",
    "vue": "3.5.16",
    "vue-chartjs": "5.3.2",
    "vue-router": "4.5.1",
    "zod": "3.25.42"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "1.0.7",
    "@biomejs/biome": "1.9.4",
    "@nuxtjs/tailwindcss": "7.0.0-beta.0",
    "@trunkio/launcher": "1.3.4",
    "@types/bun": "1.2.15",
    "@types/lodash-es": "4.17.12",
    "@types/readline-sync": "1.4.8",
    "@vitest/ui": "3.1.4",
    "commander": "14.0.0",
    "happy-dom": "17.5.6",
    "ms": "2.1.3",
    "nitro-cloudflare-dev": "0.2.2",
    "nitropack": "2.11.12",
    "readline-sync": "1.4.10",
    "rimraf": "6.0.1",
    "tailwindcss": "4.1.8",
    "vitest": "3.1.4",
    "wrangler": "4.18.0"
  },
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "bun run types && bun run nuxt build",
    "cf-typegen": "bun run wrangler types",
    "check": "bun run reset && bun run format && bun run lint && bun run typecheck && bun run test",
    "clean": "bun run rimraf .nuxt .output .wrangler *.d.ts",
    "deploy": "bun run types && bun run build && bun run wrangler deploy",
    "deploy:env": "bun run bin/deploy-env.ts",
    "dev": "bun run types && bun run nuxt dev",
    "format": "bun run biome format --write && bun run trunk fmt -a",
    "generate": "bun run types && bun run nuxt generate",
    "jwt": "bun run bin/jwt.ts",
    "kv": "bun run bin/kv.ts",
    "lint": "bun run biome check --write && bun run trunk check -a --fix",
    "packages": "bun run rimraf node_modules bun.lock && bun install",
    "postinstall": "bun run types && bun run nuxt prepare",
    "preview": "bun run preview:cloudflare",
    "preview:cloudflare": "bun run types && bun run build && wrangler dev",
    "preview:nuxt": "bun run types && bun run nuxt preview",
    "reset": "bun run clean && bun run packages && bun run build",
    "test": "bun run vitest run",
    "test:watch": "bun run vitest",
    "test:api": "bun run bin/api-test.ts",
    "test:coverage": "bun run vitest --coverage",
    "test:ui": "bun run vitest --ui",
    "typecheck": "bun run tsc --noEmit",
    "types": "bun run cf-typegen"
  },
  "trustedDependencies": [
    "@anthropic-ai/claude-code",
    "@biomejs/biome",
    "@parcel/watcher",
    "@tailwindcss/oxide",
    "better-sqlite3",
    "esbuild",
    "sharp",
    "unrs-resolver",
    "vue-demi",
    "workerd"
  ],
  "type": "module"
}
